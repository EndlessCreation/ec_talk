<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>ec_talk</display-name>
	<servlet>
		<display-name>AuthServlet</display-name>
		<servlet-name>AuthServlet</servlet-name>
		<servlet-class>kr.re.ec.talk.servlet.AuthServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AuthServlet</servlet-name>
		<url-pattern>/AuthServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<display-name>SendMessageServlet</display-name>
		<servlet-name>SendMessageServlet</servlet-name>
		<servlet-class>kr.re.ec.talk.servlet.SendMessageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SendMessageServlet</servlet-name>
		<url-pattern>/SendMessageServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<display-name>RequestNewMessagesServlet</display-name>
		<servlet-name>RequestNewMessagesServlet</servlet-name>
		<servlet-class>kr.re.ec.talk.servlet.RequestNewMessagesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RequestNewMessagesServlet</servlet-name>
		<url-pattern>/RequestNewMessagesServlet</url-pattern>
	</servlet-mapping>
</web-app>